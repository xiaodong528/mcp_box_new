​# Memo 应用配置
MEMO_DB_PATH=/path/to/memo.db

# MCP Box - E2B 沙箱配置
# 必填: E2B Jupyter 服务器地址
E2B_JUPYTER_HOST=your-e2b-host-ip
# 可选: E2B Jupyter 端口 (默认 49999)
E2B_JUPYTER_PORT=49999
# 可选: 启用调试日志 (默认 false)
E2B_DEBUG=false
# 必填: E2B API 密钥
E2B_API_KEY=your-e2b-api-key

# MCP Box - PostgreSQL 数据库配置
# 数据库主机地址
DB_HOST=10.19.88.9
# 数据库端口
DB_PORT=5432
# 数据库名称
DB_NAME=mcpbox
# 数据库用户名
DB_USER=mcpbox
# 数据库密码
DB_PASSWORD=mcpbox